CREATE TABLE MANAGER (
	MAN_ID VARCHAR(10) PRIMARY KEY,
    MAN_PW VARCHAR(30) NOT NULL,
    MAN_NAME VARCHAR(30),
    MAN_DEPARTMENT VARCHAR(10),
    MAN_PHONE INTEGER(11),
    MAN_EMAIL VARCHAR(30)
);
CREATE TABLE COMPANY (
	COM_ID VARCHAR(10) PRIMARY KEY,
    COM_PW VARCHAR(30) NOT NULL,
    COM_NAME VARCHAR(30),
    COM_DEPARTMENT VARCHAR(10),
    COM_PHONE INTEGER(11),
    COM_EMAIL VARCHAR(30),
    COM_TYPE VARCHAR(10)
);
CREATE TABLE STUDENT (
	STU_ID VARCHAR(10) PRIMARY KEY,
    STU_PW VARCHAR(30) NOT NULL,
    STU_NAME VARCHAR(30),
    STU_PHONE INTEGER(13),
    STU_EMAIL VARCHAR(30)
);