  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.example.project_wizian2.manager.service.ManagerMapper">
  	<select id="getList" resultType="ManagerResumeListVO">
  		select * from manager where yn = "t"
  	</select>
  	
<!--   	<select id="umList" resultType="ResumeVO">
  		select * from manager
  	</select> -->
  	
  	<update id="updatePostStatus">
  		update manager set yn = #{yn} where title = #{title}
  	</update>
  	
  	<select id="updatePost" resultType="ManagerResumeListVO">
  		select * from manager where yn = "y"
  	</select>
  	
  	
  </mapper>